    "FAQQuestion1": "What is a coronavirus ?",
    "@FAQQuestion1": {
        "type": "text",
        "source_text": "Qu'est-ce qu'un coronavirus ?"
    },
    "FAQQuestion2": "What is COVID-19 ?",
    "@FAQQuestion2": {
        "type": "text",
        "source_text": "Qu'est-ce que la COVID-19 ?"
    },
    "FAQQuestion3": "What are the symptoms of COVID-19 ?",
    "@FAQQuestion3": {
        "type": "text",
        "source_text": "Quels sont les symptômes de la COVID-19 ?"
    },
    "FAQQuestion4": "How is COVID-19 spreading ?",
    "@FAQQuestion4": {
